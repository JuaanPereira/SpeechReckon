<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="MainFrame">
    <property name="height">200</property>
    <property name="width">450</property>
    <layout>
      <property name="column">2</property>
      <property name="propagate">True</property>
      <property name="row">3</property>
      <rows>
        <row id="0">
          <property name="weight">0</property>
        </row>
        <row id="1">
          <property name="weight">8</property>
        </row>
      </rows>
    </layout>
    <child>
      <object class="tk.Label" id="lblTitle">
        <property name="text" translatable="yes">SpeechReckon - a speech recognizing tool.
</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="lblYouSaid">
        <property name="compound">top</property>
        <property name="cursor">arrow</property>
        <property name="text" translatable="yes">You said:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="btnListen">
        <property name="command">onListenClicked</property>
        <property name="default">active</property>
        <property name="pady">0</property>
        <property name="repeatdelay">0</property>
        <property name="text" translatable="yes">Listen</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="lblSpokenPhrase">
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="rowspan">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="lblTitleTrans">
        <property name="text" translatable="yes">Translation:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="lblTranslated">
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
